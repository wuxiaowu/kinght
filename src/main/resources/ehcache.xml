<?xml version="1.0" encoding="UTF-8"?><ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true" monitoring="autodetect">		<diskStore path="java.io.tmpdir"/>    <defaultCache            maxElementsInMemory="10000"            eternal="false"            timeToIdleSeconds="600"            timeToLiveSeconds="1200"            overflowToDisk="false"            diskPersistent="false"            diskExpiryThreadIntervalSeconds="1800"            memoryStoreEvictionPolicy="LRU"            />	 <cache name="productCache"           maxElementsInMemory="5000"           eternal="false"           overflowToDisk="false"           timeToIdleSeconds="600"           timeToLiveSeconds="1000"           memoryStoreEvictionPolicy="LFU">     </cache>    <cache name="productStatusCache"           maxElementsInMemory="10000"           eternal="false"           overflowToDisk="false"           timeToIdleSeconds="600"           timeToLiveSeconds="1200"           memoryStoreEvictionPolicy="LFU">    </cache>    <cache name="productCommonCache"           maxElementsInMemory="10000"           eternal="false"           overflowToDisk="false"           timeToIdleSeconds="36000"           timeToLiveSeconds="60000"           memoryStoreEvictionPolicy="LFU">    </cache>    <cache name="productStoreCache"           maxElementsInMemory="10000"           eternal="false"           overflowToDisk="false"           timeToIdleSeconds="300"           timeToLiveSeconds="600"           memoryStoreEvictionPolicy="LFU">    </cache>    <cache name="orderCache"           maxElementsInMemory="10000"           eternal="false"           overflowToDisk="false"           timeToIdleSeconds="100"           timeToLiveSeconds="180"           memoryStoreEvictionPolicy="LFU">    </cache>    <cache name="orderDetailCache"           maxElementsInMemory="5000"           eternal="false"           overflowToDisk="false"           timeToIdleSeconds="1200"           timeToLiveSeconds="1800"           memoryStoreEvictionPolicy="LFU">    </cache>    <cache name="customerCache"           maxElementsInMemory="10000"           eternal="false"           overflowToDisk="false"           timeToIdleSeconds="300"           timeToLiveSeconds="600"           memoryStoreEvictionPolicy="LFU">    </cache>    <cache name="promoCache"           maxElementsInMemory="10000"           eternal="false"           overflowToDisk="false"           timeToIdleSeconds="600"           timeToLiveSeconds="1200"           memoryStoreEvictionPolicy="LFU">    </cache>    <cache name="promoCommonCache"           maxElementsInMemory="10000"           eternal="false"           overflowToDisk="false"           timeToIdleSeconds="36000"           timeToLiveSeconds="60000"           memoryStoreEvictionPolicy="LFU">    </cache>    <cache name="promoCodeCache"           maxElementsInMemory="10000"           eternal="false"           overflowToDisk="false"           timeToIdleSeconds="120"           timeToLiveSeconds="300"           memoryStoreEvictionPolicy="LFU">    </cache>    <cache name="promoCodeUseCache"           maxElementsInMemory="50000"           eternal="false"           overflowToDisk="false"           timeToIdleSeconds="600"           timeToLiveSeconds="600"           memoryStoreEvictionPolicy="LFU">    </cache>    <cache name="orderOperationUseCache"           maxElementsInMemory="50000"           eternal="false"           overflowToDisk="false"           timeToIdleSeconds="900"           timeToLiveSeconds="900"           memoryStoreEvictionPolicy="LFU">    </cache>    <cache name="sysConfigCacheShort"           maxElementsInMemory="100"           eternal="false"           overflowToDisk="false"           timeToIdleSeconds="600"           timeToLiveSeconds="1200"           memoryStoreEvictionPolicy="LFU">    </cache>    <cache name="sysConfigCache"           maxElementsInMemory="500"           eternal="false"           overflowToDisk="false"           timeToIdleSeconds="10800"           timeToLiveSeconds="14400"           memoryStoreEvictionPolicy="LFU">    </cache>    <cache name="addressCache"           maxElementsInMemory="10000"           eternal="false"           overflowToDisk="false"           timeToIdleSeconds="180"           timeToLiveSeconds="300"           memoryStoreEvictionPolicy="LFU">        <!-- 缓存监听器，监听缓存元素的过期、销毁等 -->        <cacheEventListenerFactory class="com.brainlong.ccbs.cache.AddressCacheEventListenerFactory"/>    </cache>    <cache name="commonGeoCache"           maxElementsInMemory="115200"           eternal="false"           overflowToDisk="true"           timeToIdleSeconds="86400"           timeToLiveSeconds="86400"           memoryStoreEvictionPolicy="LFU">    </cache>    <cache name="communicationCache"           maxElementsInMemory="115200"           eternal="false"           overflowToDisk="true"           timeToIdleSeconds="86400"           timeToLiveSeconds="86400"           memoryStoreEvictionPolicy="LFU">    </cache>    <cache name="coreEnumerationCache"           maxElementsInMemory="115200"           eternal="false"           overflowToDisk="true"           timeToIdleSeconds="86400"           timeToLiveSeconds="86400"           memoryStoreEvictionPolicy="LFU">    </cache>    <cache name="estimatedDeliveryTimeCache"           maxElementsInMemory="115200"           eternal="false"           overflowToDisk="true"           timeToIdleSeconds="21600"           timeToLiveSeconds="21600"           memoryStoreEvictionPolicy="LFU">    </cache>    <cache name="paymentMethodCache"           maxElementsInMemory="115200"           eternal="false"           overflowToDisk="true"           timeToIdleSeconds="86400"           timeToLiveSeconds="86400"           memoryStoreEvictionPolicy="LFU">    </cache>    <cache name="orderConfirmCache"           maxElementsInMemory="115200"           eternal="false"           overflowToDisk="true"           timeToIdleSeconds="86400"           timeToLiveSeconds="86400"           memoryStoreEvictionPolicy="LFU">    </cache></ehcache>